{
  "name": "@sorry-cypress/director",
  "version": "1.0.0-beta.10",
  "author": {
    "name": "Andrew Goldis",
    "email": "agoldis@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agoldis/sorry-cypress.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/typings",
  "scripts": {
    "dev": "run-p dev:nodemon types:watch",
    "dev:nodemon": "nodemon",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "types:watch": "tsc --noEmit --watch",
    "build:js": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "test": "jest",
    "lint": "eslint ./ --ext ts",
    "start": "node ./dist/",
    "clean": "rimraf ./dist",
    "release": "yarn version --no-git-tag-version"
  },
  "dependencies": {
    "ajv": "^6.12.5",
    "aws-sdk": "^2.518.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "clone-deep": "^4.0.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "lodash": "^4.17.19",
    "md5": "^2.2.1",
    "minio": "^7.0.15",
    "mongodb": "^3.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/body-parser": "^1.19.0",
    "@types/clone-deep": "^4.0.1",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/md5": "^2.1.33",
    "@types/minio": "^7.0.5",
    "@types/mongodb": "^3.3.1",
    "@types/node": "^14.11.2",
    "@types/uuid": "^3.4.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^25.5.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.0.3"
  }
}
