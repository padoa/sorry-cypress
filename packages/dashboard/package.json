    "dotenv": "^8.2.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "humanize-duration": "^3.23.1",
    "lodash.capitalize": "^4.2.1",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.setwith": "^4.3.2",
    "lodash.truncate": "^4.4.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
