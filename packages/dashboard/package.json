  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "dev:app": "NODE_ENV=development nodemon --watch ./server ./server/",
    "dev": "run-p dev:app generate:watch",
    "format": "prettier --write .",
    "start": "node ./server",
    "build": "npm run build:webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "build:webpack": "webpack --mode production",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:watch": "graphql-codegen --config codegen.yml --watch",
    "test": "echo \"@sorry-cypress/dashboard\"",
    "lint": "eslint ./ --ext ts,tsx",
    "release": "yarn version --no-git-tag-version"
  },
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "@popperjs/core": "^2.4.0",
    "bold-ui": "^1.0.0-beta.40",
